---
import DiscoverCard from "@components/discovercard.astro";
import CardGrid from "@components/cardgrid.astro";

import { getCollection } from "astro:content";
const collection = await getCollection("discover");
---
<CardGrid>
  {
    collection.map((item) => (
    <DiscoverCard 
      item={item}/>
    ))
  }
</CardGrid>
